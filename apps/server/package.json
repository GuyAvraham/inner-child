{
  "name": "@innch/server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "pnpm with-env next dev",
    "dev:w": "pnpm with-env next dev --turbo",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "lint:fix": "pnpm lint --fix",
    "start": "pnpm with-env next start",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.29.6",
    "@innch/api": "^0.1.0",
    "@innch/db": "^0.1.0",
    "@innch/utils": "*",
    "@t3-oss/env-nextjs": "^0.6.1",
    "@tanstack/react-query": "^4.33.0",
    "@tanstack/react-query-devtools": "^4.33.0",
    "@tanstack/react-query-next-experimental": "5.0.0-alpha.80",
    "@trpc/client": "^10.38.0",
    "@trpc/next": "^10.38.0",
    "@trpc/react-query": "^10.38.0",
    "@trpc/server": "^10.38.0",
    "ai": "^2.2.31",
    "clsx": "^2.0.0",
    "next": "^14.1.0",
    "openai-edge": "^1.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-circular-progressbar": "2.1.0",
    "superjson": "1.13.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@innch/eslint-config": "^0.2.0",
    "@innch/tailwind-config": "^0.1.0",
    "@types/node": "^18.17.11",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.47.0",
    "postcss": "^8.4.28",
    "tailwindcss": "3.3.2",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@innch/eslint-config/base",
      "@innch/eslint-config/nextjs",
      "@innch/eslint-config/react"
    ]
  }
}
